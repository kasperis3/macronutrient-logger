extends layout

block main
  h1 A list of my favorite nutrient dense foods
  button(type="submit") Sort by fat
  button(type="submit") Sort by protein
  button(type="submit") Sort by net carbohydrate
  - let keys = Object.keys(awesomeFoods)
  section
    each key in keys
      hr 
      p #{key} 
      - let nutrients = Object.keys(awesomeFoods[key])
      each nute in nutrients
        li #{nute} #{awesomeFoods[key][nute]}

  block footer
    a(href="/dashboard") Back to dashboard